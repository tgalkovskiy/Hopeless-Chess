    \u0443\u0434\u0430\u043B\u043E\u0441\u044C \u0441\u0431\u0435\u0436\u0430\u0442\u044C.
    \u0414\u043E\u043B\u0433\u043E\u0435 \u0432\u0440\u0435\u043C\u044F \u043E\u043D
    \u0431\u0440\u043E\u0434\u0438\u043B \u0431\u0435\u0437 \u0435\u0434\u044B \u0438
    \u0432\u043E\u0434\u044B, \u043E\u0431\u0440\u0430\u0449\u0430\u044F \u0441\u0435\u0431\u044F
    \u0432\u0437\u043E\u0440\u044B \u043F\u0440\u043E\u0445\u043E\u0436\u0438\u0445
    \u0438 \u0442\u0435\u0440\u043F\u044F \u0443\u043D\u0438\u0436\u0435\u043D\u0438\u044F.\n\u0421\u043F\u0443\u0441\u0442\u044F
